{
  "name": "smtm-landing",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "cd apps/web && next dev",
    "dev:tg": "tsx apps/telegram-bot/index.ts",
    "dev": "node scripts/dev.mjs",
    "build": "npm run build:web",
    "build:web": "cd apps/web && next build",
    "build:bot": "echo 'Bot uses tsx, no build needed'",
    "start": "cd apps/web && next start",
    "start:bot": "tsx apps/telegram-bot/index.ts",
    "lint": "cd apps/web && next lint",
    "test:setup": "tsx packages/data/test-setup.ts",
    "example:basic": "tsx packages/data/examples/basic-usage.ts",
    "example:whales": "tsx packages/data/examples/whale-tracker.ts",
    "test:data": "npm run test:setup"
  },
  "dependencies": {
    "@polymarket/real-time-data-client": "^1.4.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "0.7.0",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.3.1",
    "lucide-react": "0.428.0",
    "next": "14.2.5",
    "node-cron": "^4.2.1",
    "pino": "^10.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.5.2",
    "telegraf": "^4.16.3",
    "tsx": "^4.19.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/node-cron": "^3.0.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.17",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^13.0.0",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
